@model IEnumerable<ProjectAkhirPAW.Models.Memilih>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.JumlahPesanan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdMenuNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPelangganNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.JumlahPesanan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdMenuNavigation.IdMenu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPelangganNavigation.IdPelanggan)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdMemilih">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdMemilih">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdMemilih">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
