@model IEnumerable<ProjectAkhirPAW.Models.Membayar>

@{
    ViewData["Title"] = "Index";
}

<h2>Membayar</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NotaPembayaran)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdKasirNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPelangganNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NotaPembayaran)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdKasirNavigation.IdKasir)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPelangganNavigation.IdPelanggan)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTransaksi">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdTransaksi">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdTransaksi">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
